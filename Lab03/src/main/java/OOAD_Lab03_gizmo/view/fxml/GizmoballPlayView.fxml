            fx:controller="OOAD_Lab03_gizmo.controller.PlayViewController"
            fx:id="playgroundRoot"
            xmlns:fx="http://javafx.com/fxml/1">


    <right>
        <VBox>
            <BorderPane.margin>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
